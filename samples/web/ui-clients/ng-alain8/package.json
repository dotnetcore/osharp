{
  "name": "osharpns",
  "version": "3.1.2",
  "private": true,
  "scripts": {
    "analyze": "npm run color-less && node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
    "build": "npm run color-less && node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod",
    "color-less": "node scripts/color-less.js",
    "e2e": "ng e2e",
    "hmr": "ng serve -c=hmr",
    "icon": "ng g ng-alain:plugin icon",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "lint:ts": "tslint -c tslint.json \"src/**/*.ts\" --fix",
    "ng": "ng",
    "start": "npm run color-less && ng s -o --port 4201",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage --watch=false",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "ignore": [
      "src/assets/*"
    ],
    "linters": {
      "src/**/*.ts": [
        "npm run lint:ts",
        "git add"
      ],
      "src/**/*.less": [
        "npm run lint:style",
        "git add"
      ]
    }
  },
  "resolutions": {
    "@babel/preset-env": "^7.8.7"
  },
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@antv/data-set": "0.10.2",
    "@antv/g2": "3.5.13",
    "@antv/g2-plugin-slider": "2.1.1",
    "@delon/abc": "8.9.0",
    "@delon/acl": "8.9.0",
    "@delon/auth": "8.9.0",
    "@delon/cache": "8.9.0",
    "@delon/chart": "8.9.0",
    "@delon/form": "8.9.0",
    "@delon/mock": "8.9.0",
    "@delon/theme": "8.9.0",
    "@delon/util": "8.9.3",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "ajv": "6.12.6",
    "heartbeats": "5.0.1",
    "jquery": "3.4.1",
    "linqts": "1.12.6",
    "moment": "2.24.0",
    "ng-alain": "8.9.3",
    "ng-zorro-antd": "8.5.2",
    "rxjs": "6.4.0",
    "rxjs-compat": "6.5.4",
    "screenfull": "4.2.1",
    "sort-package-json": "1.40.0",
    "tslib": "1.11.1",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.14",
    "@angularclass/hmr": "2.1.3",
    "@delon/testing": "8.9.3",
    "@angular/cli": "8.3.29",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/node": "8.9.5",
    "@types/jasmine": "3.9.1",
    "@types/jasminewd2": "2.0.8",
    "antd-theme-generator": "1.1.9",
    "codelyzer": "5.2.2",
    "husky": "3.1.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.6",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "less-bundle-promise": "1.0.7",
    "lint-staged": "8.2.1",
    "ng-alain-codelyzer": "0.0.1",
    "prettier": "1.19.1",
    "prettier-stylelint": "0.4.2",
    "protractor": "5.4.3",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.3.0",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "18.3.0",
    "stylelint-declaration-block-no-ignored-properties": "2.3.0",
    "stylelint-order": "3.1.1",
    "ts-node": "7.0.1",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-language-service": "0.9.9",
    "typescript": "3.5.3"
  }
}
